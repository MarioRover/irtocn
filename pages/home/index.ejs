<!-- Contact Section -->

<section id="contact" class="parallax-section">
     <div class="container">
          <div class="row">

               <div class="col-md-6 col-sm-12">
                    <div class="contact-form">
                         <div class="wow fadeInUp section-title" data-wow-delay="0.2s">
                              <% if(__('lang') == 'en') { %>
                                   <h1 class="color-white"><%- contact.headerEN %></h1>
                              <% } %>
                              <% if(__('lang') == 'cn') { %>
                                   <h1 class="color-white"><%- contact.headerCN %></h1>
                              <% } %>
                              <% if(__('lang') == 'fa') { %>
                                   <h1 class="color-white"><%- contact.headerFA %></h1>
                              <% } %>
                         </div>

                         <div id="contact-form">
                              <form class="Message">
                                   <div class="error-box alert alert-warning hidden"></div>
                                   <div class="wow fadeInUp" data-wow-delay="1s">
                                        <input name="fullname" type="text" class="form-control" id="fullname" placeholder="<%= __('home.contact.name') %>">
                                   </div>
                                   <div class="wow fadeInUp" data-wow-delay="1.2s">
                                        <input name="email" type="email" class="form-control" id="email" placeholder="<%= __('home.contact.email') %>">
                                   </div>
                                   <div class="wow fadeInUp" data-wow-delay="1.4s">
                                        <textarea name="message" rows="5" class="form-control" id="message" placeholder="<%= __('home.contact.message') %>"></textarea>
                                   </div>
                                   <div class="wow fadeInUp col-md-6 col-sm-8" data-wow-delay="1.6s">
                                        <input name="submit" type="submit" class="form-control" id="submit" value="<%= __('home.contact.button') %>">
                                        <input type="hidden" name="recaptcha" value="">
                                   </div>
                              </form>
                         </div>

                    </div>
               </div>

               <div class="col-md-3 col-sm-6">
                    <% if(isEmpty(contact.image)) { %>
                         <div class="background-image contact-img"></div> 
                    <% } else { %>
                         <div class="background-image contact-img" style="background: url('<%= contact.image.destination %>') no-repeat;"></div>  
                    <% } %>     
               </div>

               <div class="bg-dark col-md-3 col-sm-6">
                    <div class="contact-thumb">
                         <div class="wow fadeInUp contact-info" data-wow-delay="0.6s">
                              <% if(__('lang') == 'en') { %>
                                   <%- contact.descEN %>
                              <% } %>
