<link rel="stylesheet" href="/dist/<%= manifest.bundle[0] %>" as="style">
<style>
     body {font-family: "OpenSans";}
</style>
<% if(__('lang') == 'fa') { %>
<link rel="stylesheet" href="/css/rtl.css"> 
<style>
     body {font-family: "iransans";}
</style>
<% } %>
<div class="navbar navbar-fixed-top custom-navbar" role="navigation">
     <div class="container">

          <!-- navbar header -->
          <div class="navbar-header">
               <button class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon icon-bar"></span>
                    <span class="icon icon-bar"></span>
                    <span class="icon icon-bar"></span>
               </button>
               <a href="#" class="navbar-brand"><%= info.siteName %></a>
          </div>

          <div class="collapse navbar-collapse">
               <ul class="nav navbar-nav navbar-right">
                    <li><a href="#home" class="smoothScroll"><%= __('home.navbar.home')%></a></li>
                    <li><a href="#about" class="smoothScroll"><%= __('home.navbar.aboutMe')%></a></li>
                    <li><a href="#experience" class="smoothScroll"><%= __('home.navbar.experiences')%></a></li>
                    <li><a href="#quotes" class="smoothScroll"><%= __('home.navbar.testimonial')%></a></li>
                    <li><a href="#contact" class="smoothScroll"><%= __('home.navbar.contact')%></a></li>
               </ul>
          </div>

     </div>
</div>


<!-- Home Section -->

<section id="home" class="parallax-section">
     <div class="container">
          <div class="row">

               <div class="col-md-6 col-sm-6">
                    <% if(isEmpty(home.image)) { %>
                         <div class="home-img"></div>
                    <% } else { %>
                         <div class="home-img" style="background-image: url('<%= testimonial.image.destination %>')"></div>
                    <% } %>     
               </div>

               <div class="col-md-6 col-sm-6">
                    <div class="home-thumb">
                         <div class="section-title">
                              <% if(__('lang') == 'en') { %>
                                   <%- home.descEN %>
                                   <a href="#about" class="wow fadeInUp smoothScroll section-btn btn btn-success" data-wow-delay="1.4s">Get Started</a>
                              <% } %>
                              <% if(__('lang') == 'cn') { %>
                                   <%- home.descCN %>
                                   <a href="#about" class="wow fadeInUp smoothScroll section-btn btn btn-success" data-wow-delay="1.4s">入门</a>
                              <% } %>
                              <% if(__('lang') == 'fa') { %>
                                   <%- home.descFA %>
                                   <a href="#about" class="wow fadeInUp smoothScroll section-btn btn btn-success" data-wow-delay="1.4s">شروع کنید</a>
                              <% } %>
                         </div>
                    </div>
               </div>


          </div>
     </div>
</section>


<!-- About Section -->

<section id="about" class="parallax-section">
     <div class="container">
          <div class="row">

               <div class="col-md-6 col-sm-12">
                    <div class="about-thumb">
                         <div class="wow fadeInUp" data-wow-delay="0.4s">
                              <% if(__('lang') == 'en') { %>
                                   <%- about.descEN %>
                              <% } %>
                              <% if(__('lang') == 'cn') { %>
                                   <%- about.descCN %>
                              <% } %>
                              <% if(__('lang') == 'fa') { %>
                                   <%- about.descFA %>
                              <% } %>   
                         </div>
                    </div>
               </div>

               <div class="col-md-3 col-sm-6">
                    <% if(isEmpty(about.image)) { %>
                         <div class="background-image about-img"></div>
                    <% } else { %>
                         <div class="background-image about-img" style="background-image: url('<%= about.image.destination %>')"></div>     
                    <% } %>     
               </div>

               <div class="bg-yellow col-md-3 col-sm-6">
                    <div class="skill-thumb">
                         <div class="wow fadeInUp section-title color-white" data-wow-delay="1.2s">
                              <h1><%= __("skill.header")%></h1>
                         </div>

                         <div class=" wow fadeInUp skills-thumb" data-wow-delay="1.6s">
                              <% if(!isEmpty(skills)) { %>
                                   <% skills.forEach(skill => { %>

                                        <% if(__('lang') == 'en') { %>
                                             <strong><%= skill.skillEN %></strong>
                                             <span class="color-white pull-right"><%= skill.skillPercent %>%</span>
                                             <div class="progress">
                                                  <div class="progress-bar progress-bar-primary" role="progressbar" aria-valuenow="<%= skill.skillPercent %>" aria-valuemin="0" aria-valuemax="100"></div>
                                             </div>
                                        <% } %>
                                        <% if(__('lang') == 'cn') { %>
                                             <strong><%= skill.skillCN %></strong>
                                             <span class="color-white pull-right"><%= skill.skillPercent %>%</span>
                                             <div class="progress">
                                                  <div class="progress-bar progress-bar-primary" role="progressbar" aria-valuenow="<%= skill.skillPercent %>" aria-valuemin="0" aria-valuemax="100"></div>
                                             </div>
                                        <% } %>
                                        <% if(__('lang') == 'fa') { %>
                                             <strong><%= skill.skillFA %></strong>
                                             <span class="color-white pull-right"><%= skill.skillPercent %>%</span>
                                             <div class="progress">
                                                  <div class="progress-bar progress-bar-primary" role="progressbar" aria-valuenow="<%= skill.skillPercent %>" aria-valuemin="0" aria-valuemax="100"></div>
                                             </div>
                                        <% } %>
                                        
                                   <% }) %>
                              <% } %>
                         </div>

                    </div>
               </div>

          </div>
     </div>
</section>


